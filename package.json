{
  "name": "@wopjs/tsur",
  "private": true,
  "version": "0.1.6",
  "description": "TypeScript goodies inspired by Rust.",
  "repository": "wopjs/tsur",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./patches": {
      "types": "./dist/patches.d.mts",
      "require": "./dist/patches.js",
      "import": "./dist/patches.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "patches": [
        "dist/patches.d.ts"
      ]
    }
  },
  "sideEffects": [
    "**/patches.*",
    "**/patches/*"
  ],
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && echo 'Run the npm publish command inside `dist`.' && exit 1",
    "lint": "eslint && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "ts-check": "tsc --noEmit",
    "docs": "typedoc --options typedoc.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage --coverage.include=src/**",
    "test:ci": "tsc --noEmit && vitest --coverage --coverage.reporter=lcov --coverage.include=src/**",
    "build": "tsup",
    "release": "commit-and-tag-version"
  },
  "keywords": [
    "typescript",
    "rust",
    "monad",
    "option",
    "result",
    "some",
    "none",
    "ok",
    "err",
    "filterMap"
  ],
  "author": "CRIMX <straybugs@gmail.com> (https://github.com/crimx/)",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.7",
    "@wopjs/eslint-config": "^0.1.12",
    "@wopjs/gzip-size": "^0.1.0",
    "commit-and-tag-version": "^12.5.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "vitest": "^3.0.4"
  }
}
